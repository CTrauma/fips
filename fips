#!/usr/bin/env python
'''
fips build system main script
'''

import sys
from mod.util import log
from mod.verbs import diag
try :
    import yaml 
except ImportError:
    log.error("could not import 'yaml', run 'pip --user install PyYAML' to install")

verbs = {
    'diag': diag
}

if len(sys.argv) <= 1:
    print('FIXME: help')
else :
    verb = sys.argv[1]
    if verb in verbs :
        verbs[verb].run(sys.argv)
    else :
        log.error("unknown verb '{}'".format(verb))



            






